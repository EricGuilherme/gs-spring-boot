package br.bluesoft.desafio.modelo;

public class Livro {
	
	private String nome;
	private int voto;
	
	public String getNome() {
		return nome;
	}
	public void setNome(String nome) {
		this.nome = nome;
	}
	public int getVoto() {
		return voto;
	}
	public void setVoto(int voto) {
		this.voto = voto;
	}

}
